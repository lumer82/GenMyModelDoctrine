[module uml2doctrine('http://www.eclipse.org/uml2/2.0.0/UML')/]

[template public generate(class : Class) post (trim())]
[comment @main /]
[file (name+'.dcm.yml', false, 'UTF-8')]
[name/]:
	type: entity
	table: [class.name.toUpper()/]
	readOnly: true
	id:
		id:
			type: integer
			generator:
				strategy: AUTO
	fields:
	[for (property : Property | class.attribute)]
		[property.name/]:
			type: [property.type.name/]
	[/for]	
[/file]
[/template]